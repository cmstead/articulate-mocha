{
	"name": "articulate-mocha",
	"displayName": "articulate-mocha",
	"description": "Test how you talk -- write update Mocha tests faster and easier",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
	],
	"main": "./extension.js",
	"contributes": {
		"languages": [
			{
				"id": "javascript",
				"extensions": [
					"js"
				],
				"aliases": [
					"JS"
				]
			}
		],
		"commands": [
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/articulate-mocha.json"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
